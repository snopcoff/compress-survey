<p id="notice"><%= notice %></p>

<h1>Survey Result</h1>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Question 1</th>
      <th>Question 2</th>
      <th>Question 3</th>
      <th>Question 4</th>
      <th>Question 5</th>
      <th>Total</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <% if !user.is_admin %>
        <tr>
          <td><%= user.name %></td>
          <% user.questions.each do |question| %>
            <td><%= question.answers.find_by(user_id: user.id).mark %></td>
          <% end %>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
